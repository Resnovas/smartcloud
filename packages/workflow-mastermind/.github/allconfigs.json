{
  "auto": {
    "enabled": true,
    "vars": {
      "branch": "auto-update",
      "branchPrefix": "chore/",
      "whitelist": [
        "dependabot[bot]",
        "dependabot-preview[bot]",
        "github-actions[bot]",
        "github-actions",
        "github-user",
        "actions-user",
        "web-flow",
        "VidendumStudios"
      ],
      "destination": "master",
      "body": ":crown: *An automated PR*",
      "labels": "skip-changelog,automerge",
      "commitOptions": "-S --sign-off",
      "pushOptions": ""
    }
  },
  "gpg": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "signingKey": true,
      "commitSign": true,
      "tagSign": true,
      "pushSign": true
    }
  },
  "projects": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "cardsConfig": "labeler-card.yml",
      "useOrg": "false ",
      "devOps": "1",
      "devOpsColumn": "Requested",
      "issues": "2",
      "issuesColumn": "New",
      "comOps": "3",
      "comOpsColumn": "Accepted"
    }
  },
  "cla": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "path": ".github/cla.json",
      "docs": "docs/getting-started/contributing/cla.md",
      "label": true,
      "prtitle": "chore: CLA Management"
    }
  },
  "periodic": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "path": ".github/labeler-path.yml"
    }
  },
  "stale": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "issue-message": "This issue has been automatically marked as stale because it has not had recent activity. It will be closed if no further activity occurs. Thank you for your contributions.",
      "issue-label": "Status - Stale",
      "pr-message": "Stale pull request message",
      "pr-label": "Status - Abandoned",
      "exempt-issue-labels": "security vulnerability",
      "days-before-stale": "30",
      "days-before-close": "5"
    }
  },
  "profanity": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true
    }
  },
  "title": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "path": ".github/labeler-title.yml",
      "labelVersioned": "0"
    }
  },
  "sponsor": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "label": "Sponsor Request ❤️"
    }
  },
  "issueBranch": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true
    }
  },
  "wip": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "label": true
    }
  },
  "semantic": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true
    }
  },
  "size": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "xs_label": "Size - XS",
      "s_label": "Size - S",
      "m_label": "Size - M",
      "l_label": "Size - L",
      "xl_label": "Size - XL",
      "xxl_label": "Size - XXL"
    }
  },
  "path": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "config": ".github/labeler-path.yml"
    }
  },
  "branch": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "config": ".github/labeler-branch.yml"
    }
  },
  "conflict": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "master": true,
      "pull": true,
      "dirty": "Needs rebase"
    }
  },
  "approve": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true
    }
  },
  "merge": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "labels": "automerge",
      "deleteLabels": "automerge",
      "method": "squash",
      "forks": true,
      "retries": 6,
      "sleep": 5000,
      "delete": true
    }
  },
  "release": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "major": "Versioning - Major",
      "minor": "Versioning - Minor",
      "patch": "Versioning - Patch"
    }
  },
  "changelog": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "config": "docs/information/changelog.md",
      "pullRequests": true,
      "prWoLabels": "skip-changelog",
      "breakingLabels": "Versioning - BREAKING",
      "enhancementLabels": "Type - Enhancement, Type - Feature",
      "bugLabels": "Type - Fix, Bug - Fixed",
      "deprecatedLabels": "Type - Decrecated",
      "removedLabels": "Type - Removal",
      "securityLabels": "security fix",
      "excludeLabels": "skip-changelog",
      "stripGeneratorNotice": "true"
    }
  },
  "copy": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "targetBranch": "master"
    }
  },
  "wiki": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": false,
      "docsToWiki": true,
      "docsToWikiSource": "docs",
      "docsToWikiDestination": "wiki",
      "docsToRepo": true,
      "docsToRepoSource": "docs",
      "docsToRepoDestination": ".",
      "docsToRepoSuffix": "_wiki",
      "wikiToDocs": true,
      "wikiToDocsPRtitle": "docs: updating wikiToDocs",
      "wikiToRepo": true
    }
  },
  "labels": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "config": ".github/labels.yml",
      "skipDelete": false
    }
  },
  "inclusive": {
    "secret": false,
    "enabled": true,
    "vars": {
      "enabled": true,
      "org": "owner",
      "team": "Community",
      "comment": "Well done on getting your first Pull Request merged by our reviewers. The maintainers are proud of you!We would like to invite you to become a member of our github community team, which will allow you to get more access to our services & access to contributor only perks. Our bot should have already sent the invite. We look forward to you joining the team."
    }
  },
  "syncNode": {
    "enabled": true,
    "vars": {
      "enabled": true,
      "direction": "overwrite-github",
      "approve": true,
      "commitMessagePrefix": "chore:",
      "description": true,
      "homepage": true,
      "keywords": true
    }
  },
  "debt": {
    "enabled": true,
    "vars": {
      "enabled": true
    }
  },
  "jest": {
    "enabled": true,
    "vars": {
      "enabled": true,
      "command": "npm run test:coverage"
    }
  },
  "sentry": {
    "enabled": true,
    "vars": {
      "enabled": true,
      "org": "",
      "project": ""
    }
  },
  "command": {
    "enabled": true,
    "vars": {
      "settings": [
        {
          "command": "help",
          "permission": "none",
          "issue_type": "both"
        },
        {
          "command": "rebase",
          "permission": "admin",
          "issue_type": "pull-request"
        }
      ]
    }
  }
}
