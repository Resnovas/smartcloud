{
	"name": "@resnovas/smartcloud",
	"version": "1.0.0-beta.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/resnovas/smartcloud.git"
	},
	"homepage": "https://smartcloud.gg/",
	"bugs": {
		"email": "bugs.smartcloud@resnovas.com",
		"url": "https://github.com/resnovas/smartcloud/issues"
	},
	"author": {
		"name": "Resnovas",
		"email": "smartcloud@resnovas.com",
		"url": "http://resnovas.com"
	},
	"contributors": [
		"Contributors can be found in 'Readme.md'"
	],
	"license": "GPL-3.0-or-later",
	"private": false,
	"main": "lib/index.js",
	"typings": "./lib/index.d.ts",
	"type": "module",
	"eslintConfig": {
		"extends": [
			"xo",
			"xo-typescript"
		]
	},
	"xo": {
		"rules": {
			"semicolon": 0,
			"import/extensions": 0,
			"import/no-cycle": [
				2,
				{
					"maxDepth": 1
				}
			]
		}
	},
	"directories": {
		"lib": "./lib",
		"doc": "./docs",
		"test": "./__tests__",
		"typing": "./lib"
	},
	"keywords": [
		"actions",
		"node",
		"setup"
	],
	"scripts": {
		"package": "npm run tsc && ncc build --source-map --target es6",
		"run": "npm run tsc && node lib/index.js",
		"schema": "typescript-json-schema ./tsconfig.json Runners -o schema.json  --propOrder --esModuleInterop --defaultProps --titles --topRef --refs --required --tsNodeRegister",
		"tsc": "tsc -p tsconfig.json",
		"postschema": "typescript-json-schema ./tsconfig.json Labels -o schema/labels.json  --propOrder --defaultProps --titles --topRef --esModuleInterop --refs --required --tsNodeRegister",
		"preschema": "typescript-json-schema ./tsconfig.json Config -o schema/configs.json  --propOrder --defaultProps --titles --topRef --esModuleInterop --refs --required --tsNodeRegister",
		"start": "xo --fix && gulp && all-contributors generate && git add . && yarn version --prerelease --no-git-tag-version",
		"test": "xo --fix && gulp testall"
	},
	"dependencies": {
		"@actions/core": "^1.9.1",
		"@actions/github": "^5.0.3",
		"@emotion/react": "^11.10.4",
		"@emotion/styled": "^11.10.4",
		"@mui/material": "^5.10.6",
		"@resnovas/utilities": "1.0.0-alpha.18",
		"@typescript-eslint/parser": "^5.38.0",
		"eslint-config-xo": "^0.42.0",
		"eslint-config-xo-typescript": "^0.53.0",
		"minimatch": "^5.1.0",
		"simple-git": "^3.14.0"
	},
	"devDependencies": {
		"@types/jest": "29.0.0",
		"@types/minimatch": "^5.1.2",
		"@types/node": "^18.7.15",
		"@types/nodegit": "^0.27.3",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@vercel/ncc": "^0.34.0",
		"all-contributors-cli": "^6.20.4",
		"docusaurus-plugin-typedoc": "^0.17.5",
		"gulp": "^4.0.2",
		"gulp-cli": "^2.3.0",
		"gulp-copy": "^4.0.1",
		"gulp-exec": "^5.0.0",
		"gulp-json-config": "^2.0.3",
		"gulp-json-fmt": "^1.0.1",
		"gulp-markdown-toc": "^1.1.0",
		"gulp-merge-json": "^2.1.2",
		"gulp-rename": "^2.0.0",
		"js-yaml": "^4.1.0",
		"mdinclude": "^0.7.2",
		"prettier": "^2.7.1",
		"prettier-plugin-organize-imports": "^3.1.1",
		"prettier-plugin-pkg": "^0.17.1",
		"prettier-plugin-sh": "^0.12.8",
		"prettier-standard": "^16.4.1",
		"tslib": "^2.4.0",
		"typedoc": "^0.23.14",
		"typedoc-plugin-markdown": "^3.10.4",
		"typescript": "*",
		"typescript-json-schema": "^0.54.0",
		"xo": "^0.52.3"
	}
}