{
	"name": "@resnovas/smartcloud",
	"version": "1.0.0-beta.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/resnovas/smartcloud.git"
	},
	"homepage": "https://smartcloud.gg/",
	"bugs": {
		"email": "bugs.smartcloud@resnovas.com",
		"url": "https://github.com/resnovas/smartcloud/issues"
	},
	"author": {
		"name": "Resnovas",
		"email": "smartcloud@resnovas.com",
		"url": "http://resnovas.com"
	},
	"contributors": [
		"Contributors can be found in 'Readme.md'"
	],
	"license": "GPL-3.0-or-later",
	"private": false,
	"main": "lib/index.js",
	"typings": "./lib/index.d.ts",
	"directories": {
		"lib": "./lib",
		"doc": "./docs",
		"test": "./__tests__",
		"typing": "./lib"
	},
	"keywords": [
		"actions",
		"node",
		"setup"
	],
	"scripts": {
		"dev:all": "npm run dev:package && npm run dev:format",
		"dev:debug": "node lib/index.js",
		"dev:format": "prettier --config .prettier.json --write .",
		"dev:format-check": "prettier --config .prettier.json --check .",
		"dev:package": "npm run dev:tsc && ncc build --source-map --target es6",
		"dev:run": "npm run dev:tsc && node lib/index.js",
		"dev:schema": "typescript-json-schema ./tsconfig.json Runners -o schema.json  --propOrder --esModuleInterop --defaultProps --titles --topRef --refs --required --tsNodeRegister",
		"dev:test": "jest --json --outputFile=result.json --testLocationInResults",
		"dev:tsc": "tsc -p tsconfig.json",
		"git:bump": "git add . && git commit -m \"chore: Bump Version & Changelog\" -s && git push",
		"gulp": "gulp",
		"package": "gulp package",
		"postdev:schema": "typescript-json-schema ./tsconfig.json Labels -o schema/labels.json  --propOrder --defaultProps --titles --topRef --esModuleInterop --refs --required --tsNodeRegister",
		"predev:schema": "typescript-json-schema ./tsconfig.json Config -o schema/configs.json  --propOrder --defaultProps --titles --topRef --esModuleInterop --refs --required --tsNodeRegister",
		"release": "yarn version --no-git-tag-version && npm run git:bump",
		"run": "gulp && git add . && yarn version --prerelease --no-git-tag-version && cz",
		"start": "npm run run",
		"test": "gulp testall"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog",
			"maxHeaderWidth": 100
		}
	},
	"dependencies": {
		"@actions/core": "^1.9.1",
		"@actions/github": "^5.1.0",
		"@emotion/react": "^11.10.4",
		"@emotion/styled": "^11.10.4",
		"@mui/material": "^5.10.6",
		"@resnovas/utilities": "1.0.0-alpha.18",
		"minimatch": "^5.1.0",
		"simple-git": "^3.14.0"
	},
	"devDependencies": {
		"@types/jest": "29.0.0",
		"@types/minimatch": "^5.1.2",
		"@types/node": "^18.7.15",
		"@types/nodegit": "^0.27.3",
		"@vercel/ncc": "^0.34.0",
		"all-contributors-cli": "^6.20.4",
		"docusaurus-plugin-typedoc": "^0.17.5",
		"gulp": "^4.0.2",
		"gulp-cli": "^2.3.0",
		"gulp-copy": "^4.0.1",
		"gulp-exec": "^5.0.0",
		"gulp-json-config": "^2.0.3",
		"gulp-json-fmt": "^1.0.1",
		"gulp-markdown-toc": "^1.1.0",
		"gulp-merge-json": "^2.1.2",
		"gulp-rename": "^2.0.0",
		"js-yaml": "^4.1.0",
		"mdinclude": "^0.7.2",
		"prettier": "^2.7.1",
		"prettier-plugin-organize-imports": "^3.1.1",
		"prettier-plugin-pkg": "^0.17.1",
		"prettier-plugin-sh": "^0.12.8",
		"prettier-standard": "^16.4.1",
		"tslib": "^2.4.0",
		"typedoc": "^0.23.14",
		"typedoc-plugin-markdown": "^3.10.4",
		"typescript": "^4.8.2",
		"typescript-json-schema": "^0.54.0"
	}
}
